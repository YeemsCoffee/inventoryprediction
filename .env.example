# Square API Configuration
SQUARE_ACCESS_TOKEN=your_square_access_token_here
SQUARE_APPLICATION_ID=your_square_application_id_here
SQUARE_ENVIRONMENT=production  # or 'sandbox' for testing

# Amazon RDS Database Configuration
# For PostgreSQL:
DATABASE_URL=postgresql://username:password@your-rds-endpoint.region.rds.amazonaws.com:5432/database_name
# For MySQL:
# DATABASE_URL=mysql+pymysql://username:password@your-rds-endpoint.region.rds.amazonaws.com:3306/database_name

# Example RDS endpoints:
# PostgreSQL: mydb.abc123xyz.us-east-1.rds.amazonaws.com
# MySQL: mydb.abc123xyz.us-west-2.rds.amazonaws.com

# API Configuration (Optional)
API_HOST=0.0.0.0
API_PORT=8000

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
